{
  "builds": [
    { "src": "package.json", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*).js", "dest": "/dist/$1.js" },
    { "src": "/(.*).css", "dest": "/dist/$1.css" },
    { "src": "/(.*).html", "dest": "/dist/$1.html" },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ],
  "rewrites": [
    {
      "source": "/(.*)", "destination": "/"
    }
  ]
}
